<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ToolKitMarkupProject.App">
    <Application.Resources>
        <ResourceDictionary>

            <OnPlatform x:TypeArguments="x:String" x:Key="FontAwesomeRegular">
                <On Platform="Android" Value="freeregular400.otf#Regular" />
                <On Platform="iOS" Value="freeregular400" />
            </OnPlatform>

            <OnPlatform x:TypeArguments="x:String" x:Key="FontAwesomeBrands">
                <On Platform="Android" Value="regular400.otf#Brands" />
                <On Platform="iOS" Value="regular400" />
            </OnPlatform>

            <OnPlatform x:TypeArguments="x:String" x:Key="FontAwesomeSolid">
                <On Platform="Android" Value="solid900.otf#Solid" />
                <On Platform="iOS" Value="solid900" />
            </OnPlatform>
        </ResourceDictionary>
    </Application.Resources>
</Application>